{
<!--Template for one concept:
    - draw concept in the map
    - get informations from attributs(coordinates, color, ect) -->
id: <%= concept.id %>,
label: "<%= concept.label %>",
x: <%= concept.data["x"] || 0 %>,
y: <%= concept.data["y"] || 0 %>,
shape: 'ellipse',
borderWidth: 2,
color: {
  background: "<%= concept.data["color"] %>",
  border: "<%= concept.data["color"] %>",
  hover: {
    background: '#cfe0c8',
    border: '#cfe0c8'
  },
  highlight:{
    background: "<%= concept.data["color"] %>",
    border: '#449d44'
  }
}
}